{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": ""
        }
      ]
    },
    {
      "source": "/logos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "functions": {
    "app/api/upload-artwork/route.ts": {
      "maxDuration": 30
    },
    "app/api/upload-temp-artwork/route.ts": {
      "maxDuration": 30
    },
    "app/api/kurator-tags/route.ts": {
      "maxDuration": 60
    }
  }
} 